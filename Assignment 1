#include<stdio.h>
#include<conio.h>
main()
{
    int t;
    scanf("%d",&t);
    while(t--) 
    {
        int n,i;
        scanf("%d",&n);
        int a[n],sum=0;
        for(i=0;i<n;i++) 
        {
            scanf("%d",&a[i]);
            sum=sum+a[i];
        }
        for(i=0;i<n;i++) 
        {
            printf("%d ",sum-a[i]);
        }
        printf("\n");
    }
}
